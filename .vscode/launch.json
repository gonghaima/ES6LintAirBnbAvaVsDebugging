{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch via NPM",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run-script",
        "start"
      ],
      "port": 9229
    },
    {
      "type": "node",
      "request": "launch",
      "sourceMaps": true,
      "name": "Launch Program",
      "program": "${workspaceFolder}/index.js",
      "runtimeExecutable": "${workspaceRoot}/node_modules/babel-cli/lib/babel-node.js"
    }
  ]
}