{
  "version": "0.2.0",
  "configurations": [{
    "type": "node",
    "request": "launch",
    "name": "Launch via Babel",
    "program": "${workspaceRoot}/dist/index.js",
    "stopOnEntry": false,
    "runtimeExecutable": "babel-node",
    "cwd": "${workspaceRoot}",
    "runtimeArgs": ["--inspect", "--nolazy", "--debug-brk"],
    "env": {
      "NODE_ENV": "development",
      "BABEL_ENV": "debug"
    },
    "sourceMaps": true,
    "preLaunchTask": "build",
    "outFiles": ["${workspaceRoot}/dist/**.js"],
    "port": 9229
  }],
  "compounds": []
}